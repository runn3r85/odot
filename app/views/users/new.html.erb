<%= title("Register for ODOT") %>

<%= form_for(@user, builder: FoundationFormBuilder) do |f| %>

  <%= f.text_field :first_name %>
  <%= f.text_field :last_name %>
  <%= f.email_field :email %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Sign Up" %>

<% end %>